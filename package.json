{
  "name": "cem-admin",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://acm-dtu.appspot.com",
  "dependencies": {
    "apollo-link": "^0.5.4",
    "font-awesome": "^4.7.0",
    "graphql": "^0.10.5",
    "material-ui": "^0.19.1",
    "moment": "^2.18.1",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-apollo": "^1.4.15",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.2.0",
    "react-icons-kit": "^1.0.7",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.13",
    "react-transition-group": "^1.1.2",
    "reactstrap": "^4.8.0",
    "redux": "^3.7.2",
    "redux-form": "^7.0.4",
    "subscriptions-transport-ws": "^0.8.3"
  },
  "scripts": {
    "build-css":
      "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css":
      "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "eject": "react-scripts eject",
    "lint-staged": "lint-staged",
    "precommit": "npm-run-all lint-staged"
  },
  "devDependencies": {
    "eslint-config-prettier": "^2.5.0",
    "eslint-plugin-prettier": "^2.2.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.1",
    "prettier": "^1.7.0",
    "prettier-eslint": "^8.1.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "prettier --tab-width 2 --single-quote --trailing-comma all --print-with 80 --write",
      "git add"
    ]
  }
}
