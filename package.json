{
  "name": "cem-admin",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://acm-dtu.appspot.com",
  "engines": {
    "node": "8.x"
  },
  "dependencies": {
    "apisauce": "^0.14.1",
    "font-awesome": "^4.7.0",
    "graphql": "0.10.5",
    "material-ui": "^0.19.2",
    "moment": "^2.18.1",
    "node-sass-chokidar": "0.0.3",
    "prop-types": "15.6.0",
    "react": "15.6.1",
    "react-apollo": "^1.4.16",
    "react-dom": "15.6.1",
    "react-helmet": "^5.2.0",
    "react-icons-kit": "^1.0.7",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.14",
    "react-swipeable-views": "^0.12.8",
    "react-transition-group": "^2.2.1",
    "redux": "^3.7.2",
    "redux-form": "^7.0.4",
    "redux-form-material-ui": "^4.3.0",
    "redux-thunk": "^2.2.0",
    "subscriptions-transport-ws": "0.8.3"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "dev": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "eject": "react-scripts eject",
    "lint-staged": "lint-staged",
    "precommit": "npm-run-all lint-staged",
    "start": "serve -s build",
    "prestart": "npm install -g serve"
  },
  "devDependencies": {
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-prettier": "^2.3.1",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.3",
    "npm-run-all": "^4.1.1",
    "prettier": "^1.7.2",
    "prettier-eslint": "^8.2.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "prettier --tab-width 2 --single-quote --trailing-comma all --print-with 80 --write",
      "git add"
    ]
  }
}
